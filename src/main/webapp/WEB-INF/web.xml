<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


    <servlet>
           <servlet-name>login</servlet-name>
           <servlet-class>com.Photographer.LoginServlet</servlet-class>
    </servlet>
    
   <servlet-mapping>
         <servlet-name>login</servlet-name>
         <url-pattern>/log</url-pattern>
         
   </servlet-mapping>
   
   <servlet>
       <servlet-name>PackageDetails</servlet-name>
        <servlet-class>com.Photographer.PackageDetails</servlet-class>
       
   </servlet>
   
  <servlet-mapping>
         <servlet-name>PackageDetails</servlet-name>
         <url-pattern>/insert</url-pattern>
         
   </servlet-mapping>
   
   <servlet>
          <servlet-name>UpdatePhotographer</servlet-name>
           <servlet-class>com.Photographer.UpdatePhotographerServlet</servlet-class>
   </servlet>

  <servlet-mapping>
           <servlet-name>UpdatePhotographer</servlet-name>
           <url-pattern>/update</url-pattern>  
  </servlet-mapping>
  
   <servlet>
          <servlet-name>DeletePhotographer</servlet-name>
           <servlet-class>com.Photographer.DelectPhotographerServlet</servlet-class>
   </servlet>

  <servlet-mapping>
           <servlet-name>DeletePhotographer</servlet-name>
           <url-pattern>/delete</url-pattern>  
  </servlet-mapping>
  
  
</web-app>


